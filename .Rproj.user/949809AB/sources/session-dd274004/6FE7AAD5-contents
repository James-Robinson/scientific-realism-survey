# contingency test across 5 clusters with 2 levels
library(survival)
library(coin)
library("readxl")

getwdsetwd("E:/Realism_Questionaire/Scripting")
my_data <- read_excel("Phys_categorical_for_r.xlsx")

my_data$research <- as.factor(my_data$research)
my_data$exptheo <- as.factor(my_data$exptheo)
my_data$Cluster <- as.factor(my_data$Cluster)

chisq_test(my_data$Cluster~my_data$research,distribution=approximate(B=10000))

chisq_test(my_data$Cluster~my_data$exptheo,distribution=approximate(B=10000))